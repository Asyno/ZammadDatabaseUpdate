<Window x:Class="ZammadDatabaseUpdate.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZammadDatabaseUpdate"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="auto" />
            <RowDefinition Height="auto" />
            <RowDefinition Height="auto" />
        </Grid.RowDefinitions>
        <TextBox Name="Result" Text="Test:" />
        <Button Grid.Row="1" Name="btn_Test" Click="btn_Test_Click" Content="Get Users" />
        <Button Grid.Row="2" Name="btn_Upload" Click="btn_Upload_Click" Content="Add User" />
        <Button Grid.Row="3" Name="btn_updateBob" Click="btn_updateBob_Click" Content="Update Bob" />
    </Grid>
</Window>
